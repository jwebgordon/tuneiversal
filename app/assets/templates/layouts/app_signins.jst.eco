<div class="signin-container" id='soundcloud_signin'>
	<img src="/assets/soundcloud.png" class="sign-in-icon 
			<% if not Tuneiversal.currentUser.attributes.has_connected_soundcloud: %>
				<%= "signed-out" %>
			<% end %>
	">	
</div>
<div class="signin-container" id="rdio_signin">
	<img src="/assets/rdio-icon.png" class="sign-in-icon
		<% if not Tuneiversal.currentUser.attributes.has_connected_rdio: %>
				<%= "signed-out" %>
		<% end %>
	"> 
</div>